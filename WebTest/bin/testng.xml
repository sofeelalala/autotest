<?xml version="1.0" encoding="UTF-8"?>
<suite
	name="Suite"
	parallel="tests"
	thread-count="2">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
	</listeners>
	<test name="FirefoxTest">
		<parameter
			name="browser"
			value="firefox"/>
		<classes>
			<class name="com.test.TestDemo" />
		</classes>
	</test> <!-- Test -->
	<test name="ChromeTest">
		<parameter
			name="browser"
			value="chrome"/>
		<classes>
			<class name="com.test.TestDemo" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->